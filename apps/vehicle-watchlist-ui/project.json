{
  "name": "vehicle-watchlist-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/vehicle-watchlist-ui",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project vehicle-watchlist-ui --web",
  "targets": {
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vehicle-watchlist-ui:build",
        "dev": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "vehicle-watchlist-ui:build:development"
        },
        "production": {
          "buildTarget": "vehicle-watchlist-ui:build:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/vehicle-watchlist-ui"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/vehicle-watchlist-ui"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vehicle-watchlist-ui:build",
        "dev": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "vehicle-watchlist-ui:build:development"
        },
        "production": {
          "buildTarget": "vehicle-watchlist-ui:build:production",
          "dev": false
        }
      }
    }
  }
}
